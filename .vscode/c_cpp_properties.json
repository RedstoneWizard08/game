{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/external/**",
        "${workspaceFolder}/external/stb/**",
        "${workspaceFolder}/external/imgui/**",
        "${workspaceFolder}/external/imgui/backends/**"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [""],
      "defines": []
    },
    {
      "name": "linux-emcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/share/emscripten/system/**",
        "/usr/share/emscripten/system/include/**",
        "/usr/share/emscripten/system/lib/**",

        "/usr/share/emscripten/system/*",
        "/usr/share/emscripten/system/include/*",
        "/usr/share/emscripten/system/lib/*",

        "${workspaceFolder}/external/**",
        "${workspaceFolder}/external/stb/**",
        "${workspaceFolder}/external/imgui/**",
        "${workspaceFolder}/external/imgui/backends/**"
      ],
      "compilerPath": "/usr/bin/emcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [""],
      "defines": ["__EMSCRIPTEN__"]
    }
  ],
  "version": 4
}
